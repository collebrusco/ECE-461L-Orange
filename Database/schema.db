// https://github.com/collebrusco/ECE-461L-Orange/blob/main/Database/schema.db
Table users {
  id integer [primary key]
  username varchar
  password varchar
  created_at timestamp
  project_ids list
}

Table resources {
  id integer [primary key]
  capacity integer 
  availability integer
  title varchar
  type varchar
  created_at timestamp
}

Table projects {
  id integer [primary key]
  title varchar
  description varchar
  user_ids list
  creator integer // userid
  resources list // [resource id -> quantity] length: number of total resources
}



