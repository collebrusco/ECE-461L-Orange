Table users {
  _id ObjectID [primary key]
  username string
  password string
  projects Array              # ["Project 1", "Project 2", "Project 3"]
  created_at timestamp
}

Table projects {
  _id ObjectID [primary key]
  title string
  description string
  creator string              # "Alice"
  user Array                  # ["Alice", "Bob"]
  resources Object            # resource title -> checked-out quantity
                              # example: {
                              #   "HW Set 1": 10,
                              #   "HW Set 2": 20,
                              #   "HW Set 3": 0,
                              # }
}

Table resources {
  _id ObjectID [primary key]
  title string
  capacity integer 
  availability integer
  created_at timestamp
}
